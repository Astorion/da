<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">


<configuration>
	<!-- 데이터 컬럼명 A_COLUMN을 CamelCase 형태로 매핑 aColumn -->
	<settings>
		<setting name="mapUnderscoreToCamelCase" value="false" />
	</settings>
	<typeAliases>
	<!-- type별징 VO이름을 간략히 사용 -->
	<typeAlias type="com.uver.cmn.Search" alias="Search"/>
	<typeAlias type="com.uver.vo.LikeVO" alias="LikeVO"/>
	<typeAlias type="com.uver.vo.CommentVO" alias="CommentVO"/>
	<typeAlias type="com.uver.vo.ImgVO" alias="ImgVO"/>
	<typeAlias type="com.uver.vo.EventImgVO" alias="EventImgVO"/>
	
	</typeAliases>
</configuration>

