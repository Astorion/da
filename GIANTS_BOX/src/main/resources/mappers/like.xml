<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uver">
	<select id="doSelectOne" parameterType="LikeVO"
		resultType="LikeVO">
		SELECT
		c_seq,
		u_seq
		FROM
		good
		WHERE c_seq=#{commentSeq}
	</select>
	<delete id="doDelete" parameterType="LikeVO">
		DELETE FROM good
		WHERE
		c_seq
		=
		#{commentSeq}
	</delete>
	<insert id="doInsert" parameterType="LikeVO">
		INSERT INTO good (
		c_seq,
		u_seq
		) VALUES (
		#{commentSeq},
		#{userSeq}
		)
	</insert>
</mapper>